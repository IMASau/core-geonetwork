<rdf:RDF 
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#">

    <skos:ConceptScheme rdf:about="http://www.my.com/test_classification">
        <dcterms:title xml:lang="en">Test vocabulary</dcterms:title>
        <skos:hasTopConcept rdf:resource="http://www.my.com/#ocean"/>
        <skos:hasTopConcept rdf:resource="http://www.my.com/#physical_air"/>
        <skos:hasTopConcept rdf:resource="http://www.my.com/#physical_water"/>
    </skos:ConceptScheme>
    
    <!-- term with broader term with broader term -->

    <skos:Concept rdf:about="http://www.my.com/#ocean_temperature">
        <skos:inScheme rdf:resource="http://www.my.com/test_classification"/>
        <skos:prefLabel xml:lang="en">ocean temperature</skos:prefLabel>
        <skos:broader rdf:resource="http://www.my.com/#ocean"/>
        <skos:narrowMatch rdf:resource="http://www.my.com/#sea_surface_temperature"/>
    </skos:Concept>

    <skos:Concept rdf:about="http://www.my.com/#ocean">
        <skos:inScheme rdf:resource="http://www.my.com/test_classification"/>
        <skos:prefLabel xml:lang="en">ocean</skos:prefLabel>
        <skos:topConceptOf rdf:resource="http://www.my.com/test_classification"/>
        <skos:narrower rdf:resource="http://www.my.com/#ocean_temperature"/>
    </skos:Concept>

    <!-- term with two broader terms -->

    <skos:Concept rdf:about="http://www.my.com/#physical_air">
        <skos:inScheme rdf:resource="http://www.my.com/test_classification"/>
        <skos:prefLabel xml:lang="en">physical - air</skos:prefLabel>
        <skos:topConceptOf rdf:resource="http://www.my.com/test_classification"/>
        <skos:narrowMatch rdf:resource="http://www.my.com/#air_sea_flux"/>
    </skos:Concept>

    <skos:Concept rdf:about="http://www.my.com/#physical_water">
        <skos:prefLabel xml:lang="en">physical - water</skos:prefLabel>
        <skos:topConceptOf rdf:resource="http://www.my.com/test_classsification"/>
        <skos:narrowMatch rdf:resource="http://www.my.com/#air_sea_flux"/>
    </skos:Concept>

</rdf:RDF>
